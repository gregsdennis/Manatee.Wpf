<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib">
	<SolidColorBrush x:Key="{x:Type collections:IEnumerable}" Color="Green"/>
	<SolidColorBrush x:Key="{x:Type collections:ICollection}" Color="Blue"/>
	<SolidColorBrush x:Key="{x:Type collections:IList}" Color="Red"/>
</ResourceDictionary>